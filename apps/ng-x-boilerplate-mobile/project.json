{
  "name": "ng-x-boilerplate-mobile",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "prefix": "x",
  "sourceRoot": "apps/ng-x-boilerplate-mobile/src",
  "tags": ["type:app"],
  "implicitDependencies": [
    "shared-ui-base",
    "shared-ui-app-shell",
    "shared-ui-tailwindcss"
  ],
  "targets": {
    "build": {
      "executor": "@angular-devkit/build-angular:browser",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/apps/ng-x-boilerplate-mobile/browser",
        "index": "apps/ng-x-boilerplate-mobile/src/index.html",
        "main": "apps/ng-x-boilerplate-mobile/src/main.ts",
        "polyfills": ["zone.js"],
        "tsConfig": "apps/ng-x-boilerplate-mobile/tsconfig.app.json",
        "assets": [
          "apps/ng-x-boilerplate-mobile/src/favicon.ico",
          {
            "glob": "**/*",
            "input": "apps/ng-x-boilerplate-mobile/src/assets",
            "ignore": ["native/**"],
            "output": "/assets"
          },
          "apps/ng-x-boilerplate-mobile/src/manifest.webmanifest"
        ],
        "styles": [
          "node_modules/highcharts/css/highcharts.css",
          "node_modules/bootstrap-icons/font/bootstrap-icons.scss",
          "node_modules/@ionic/angular/css/core.css",
          "node_modules/@ionic/angular/css/display.css",
          "node_modules/@ionic/angular/css/padding.css",
          "node_modules/@ionic/angular/css/float-elements.css",
          "node_modules/@ionic/angular/css/text-alignment.css",
          "node_modules/@ionic/angular/css/text-transformation.css",
          "node_modules/@ionic/angular/css/flex-utils.css",
          "node_modules/@ionic/angular/css/palettes/dark.system.css",
          "apps/ng-x-boilerplate-mobile/src/styles.scss"
        ],
        "scripts": ["node_modules/jquery/dist/jquery.js"],
        "allowedCommonJsDependencies": [
          "jquery",
          "highcharts",
          "highcharts/modules/accessibility",
          "lottie-web/build/player/lottie_svg"
        ],
        "stylePreprocessorOptions": {
          "includePaths": []
        }
      },
      "configurations": {
        "production": {
          "budgets": [
            {
              "type": "initial",
              "maximumWarning": "2mb",
              "maximumError": "5mb"
            },
            {
              "type": "anyComponentStyle",
              "maximumWarning": "8kb",
              "maximumError": "16kb"
            }
          ],
          "outputHashing": "all",
          "optimization": {
            "scripts": true,
            "styles": {
              "minify": true,
              "inlineCritical": false
            },
            "fonts": true
          }
        },
        "development": {
          "buildOptimizer": false,
          "optimization": false,
          "vendorChunk": true,
          "extractLicenses": false,
          "sourceMap": true,
          "namedChunks": true,
          "fileReplacements": [
            {
              "replace": "apps/ng-x-boilerplate-mobile/src/environments/environment.ts",
              "with": "apps/ng-x-boilerplate-mobile/src/environments/environment.development.ts"
            }
          ]
        }
      },
      "defaultConfiguration": "production"
    },
    "serve": {
      "executor": "@angular-devkit/build-angular:dev-server",
      "configurations": {
        "production": {
          "proxyConfig": "apps/ng-x-boilerplate-mobile/src/proxy.conf.json",
          "buildTarget": "ng-x-boilerplate-mobile:build:production"
        },
        "development": {
          "proxyConfig": "apps/ng-x-boilerplate-mobile/src/proxy.conf.json",
          "buildTarget": "ng-x-boilerplate-mobile:build:development"
        }
      },
      "defaultConfiguration": "development",
      "continuous": true
    },
    "extract-i18n": {
      "executor": "@angular-devkit/build-angular:extract-i18n",
      "options": {
        "buildTarget": "ng-x-boilerplate-mobile:build"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"]
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "apps/ng-x-boilerplate-mobile/jest.config.ts",
        "tsConfig": "apps/ng-x-boilerplate-mobile/tsconfig.spec.json"
      }
    },
    "serve-static": {
      "executor": "@nx/web:file-server",
      "options": {
        "buildTarget": "ng-x-boilerplate-mobile:build"
      }
    },
    "server": {
      "executor": "@angular-devkit/build-angular:server",
      "options": {
        "outputPath": "dist/apps/ng-x-boilerplate-mobile/server",
        "main": "apps/ng-x-boilerplate-mobile/src/main.server.ts",
        "tsConfig": "apps/ng-x-boilerplate-mobile/tsconfig.server.json"
      },
      "configurations": {
        "production": {
          "outputHashing": "media"
        },
        "development": {
          "buildOptimizer": false,
          "optimization": false,
          "sourceMap": true,
          "extractLicenses": false,
          "vendorChunk": true,
          "fileReplacements": [
            {
              "replace": "apps/ng-x-boilerplate-mobile/src/environments/environment.ts",
              "with": "apps/ng-x-boilerplate-mobile/src/environments/environment.development.ts"
            }
          ]
        }
      },
      "defaultConfiguration": "production"
    },
    "app-shell": {
      "executor": "@angular-devkit/build-angular:app-shell",
      "options": {
        "route": "shell"
      },
      "configurations": {
        "production": {
          "browserTarget": "ng-x-boilerplate-mobile:build:production",
          "serverTarget": "ng-x-boilerplate-mobile:server:production"
        },
        "development": {
          "browserTarget": "ng-x-boilerplate-mobile:build:development",
          "serverTarget": "ng-x-boilerplate-mobile:server:development"
        }
      },
      "defaultConfiguration": "production"
    },
    "cap": {
      "executor": "@nxext/capacitor:cap",
      "options": {
        "cmd": "--help"
      }
    },
    "add": {
      "executor": "@nxext/capacitor:cap",
      "options": {
        "cmd": "add"
      },
      "configurations": {
        "ios": {
          "cmd": "add ios"
        },
        "android": {
          "cmd": "add android"
        }
      }
    },
    "copy": {
      "executor": "@nxext/capacitor:cap",
      "options": {
        "cmd": "copy"
      },
      "configurations": {
        "ios": {
          "cmd": "copy ios"
        },
        "android": {
          "cmd": "copy android"
        }
      }
    },
    "open": {
      "executor": "@nxext/capacitor:cap",
      "options": {
        "cmd": "open"
      },
      "configurations": {
        "ios": {
          "cmd": "open ios"
        },
        "android": {
          "cmd": "open android"
        }
      }
    },
    "run": {
      "executor": "@nxext/capacitor:cap",
      "options": {
        "cmd": "run"
      },
      "configurations": {
        "ios": {
          "cmd": "run ios"
        },
        "android": {
          "cmd": "run android"
        }
      }
    },
    "sync": {
      "executor": "@nxext/capacitor:cap",
      "options": {
        "cmd": "sync"
      },
      "configurations": {
        "ios": {
          "cmd": "sync ios"
        },
        "android": {
          "cmd": "sync android"
        }
      }
    },
    "update": {
      "executor": "@nxext/capacitor:cap",
      "options": {
        "cmd": "update"
      },
      "configurations": {
        "ios": {
          "cmd": "update ios"
        },
        "android": {
          "cmd": "update android"
        }
      }
    },
    "cap-build": {
      "executor": "@nxext/capacitor:cap",
      "options": {
        "cmd": "build"
      },
      "configurations": {
        "ios": {
          "cmd": "build ios"
        },
        "android": {
          "cmd": "build android"
        }
      }
    },
    "cap-assets": {
      "command": "npx @capacitor/assets generate --assetPath \"apps/ng-x-boilerplate-mobile/src/assets/native\" --iosProject \"apps/ng-x-boilerplate-mobile/ios/App\" --androidProject \"apps/ng-x-boilerplate-mobile/android\" --ios --android"
    },
    "cap-assets-android": {
      "command": "npx @capacitor/assets generate --assetPath \"apps/ng-x-boilerplate-mobile/src/assets/native\" --androidProject \"apps/ng-x-boilerplate-mobile/android\" --android"
    },
    "cap-assets-ios": {
      "command": "npx @capacitor/assets generate --assetPath \"apps/ng-x-boilerplate-mobile/src/assets/native\" --iosProject \"apps/ng-x-boilerplate-mobile/ios/App\" --ios"
    },
    "transloco-extract": {
      "command": "npx transloco-keys-manager extract --sort --project=apps/ng-x-boilerplate-mobile/project.json --output=apps/ng-x-boilerplate-mobile/src/assets/i18n --langs=extracted"
    }
  }
}
